#include <stdio.h>
#include <ctype.h>  
#include <locale.h>

int main() {
setlocale(LC_CTYPE, "RUS");
    char c;

    printf("Введите символ: ");
    scanf("%c", &c);

    
    if (isdigit(c)) {
        printf("Это цифра.\n");
    }
    
    else if (isalpha(c)) {
        printf("Это буква.\n");
    }
    
    else {
        printf("Неизвестный символ.\n");
    }

    return 0;
}
