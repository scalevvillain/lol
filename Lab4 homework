#include <stdio.h>
#include<locale.h>

int main()
{
    setlocale(LC_CTYPE, "RUS");
    float X; // Сумма, которую Юра тратит на сладости каждую неделю
    printf("Введите сумму, которую Юра тратит на сладости каждую неделю (X): ");
    scanf("%f", &X);

    // Количество недель в году
    int weeks = 52;

    // Сумма, накопленная за год
    float total = weeks * X;

    printf("Сумма, накопленная Юрой за год: %.2f рублей\n", total);

   
}
