#include <stdio.h>

int main() {
    int m, n;
    int S = 0; // Переменная для накопления суммы
    int I; // Счетчик цикла

    // Ввод начального и конечного значений
    printf("Введите начальное значение (m): ");
    scanf("%d", &m);
    printf("Введите конечное значение (n): ");
    scanf("%d", &n);

    // Проверка, что m меньше или равно n
    if (m > n) {
        printf("Начальное значение должно быть меньше или равно конечному значению.n");
        return 1;
    }

    // Цикл для вычисления суммы
    for (I = m; I <= n; I++) {
        S += I; // Сложение текущего значения I к S
    }

    // Вывод результата
    printf("Сумма целых чисел от %d до %d равна %dn", m, n, S);

    return 0;
}
